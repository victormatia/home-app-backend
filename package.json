{
  "name": "home-app-backend",
  "version": "1.0.0",
  "description": "An API Rest for a rental app",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r ts-node/register src/tests/**/*.test.ts --exit -t 20000",
    "dev": "tsnd --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "npm run build && node /build/server.js",
    "db:migrate": "npx prisma migrate dev",
    "db:seed": "ts-node prisma/seeders/base.ts",
    "db:reset": "npx prisma db push --force-reset  && npm run db:seed",
    "pre:dev": "npm run db:reset && npm run dev"
  },
  "keywords": [],
  "author": "Victor Matias",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "chai": "^4.3.7",
    "chai-http": "^4.4.0",
    "eslint": "^8.47.0",
    "mocha": "^10.2.0",
    "prisma": "^5.1.1",
    "sinon": "^15.2.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.0"
  }
}
